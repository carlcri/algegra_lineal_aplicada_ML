    "def render_image(U, D, V, numero_valores_singulares=1):\n",
    "    \n",
    "    i = numero_valores_singulares\n",
    "    imagen= np.matrix(U[:, :i]) * np.diag(D[:i]) * np.matrix(V[:i,:])\n",
    "\n",
    "    return imagen"
